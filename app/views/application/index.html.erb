<div class="row">
  <div class="col">
    <%= render 'screener/coins' %>
  </div>
</div>
<div class="row justify-content-end">
  <div class="col">
    <% cache @last_update, expires_in: 4.minutes do %>
      <small class="text-light">Last update <%= time_ago_in_words(@last_update) %> ago.</small>
    <% end %>
  </div>
</div>